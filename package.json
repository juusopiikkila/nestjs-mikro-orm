{
    "name": "nestjs-mikro-orm",
    "version": "0.0.1",
    "description": "",
    "author": "",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "prebuild": "rimraf dist && tsc --emitDeclarationOnly -d --outDir ./dist && rm dist/*.d.ts",
        "build": "yarn prebuild && nest build",
        "start": "nest start",
        "start:dev": "yarn prebuild && TZ=UTC nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
        "mikro-orm": "ts-node -r tsconfig-paths/register ./node_modules/.bin/mikro-orm"
    },
    "mikro-orm": {
        "configPaths": [
            "./src/mikro-orm.config.ts",
            "./mikro-orm.config.js"
        ]
    },
    "dependencies": {
        "@mikro-orm/cli": "^6.2.2",
        "@mikro-orm/core": "^6.2.2",
        "@mikro-orm/migrations": "^6.2.2",
        "@mikro-orm/mysql": "^6.2.2",
        "@mikro-orm/nestjs": "^5.2.3",
        "@mikro-orm/reflection": "^6.2.2",
        "@mikro-orm/seeder": "^6.2.2",
        "@nestjs/common": "^10.0.0",
        "@nestjs/config": "^3.2.2",
        "@nestjs/core": "^10.0.0",
        "@nestjs/graphql": "^12.1.1",
        "@nestjs/platform-express": "^10.0.0",
        "change-case": "^4.1.2",
        "class-validator": "^0.14.1",
        "dotenv": "^16.4.5",
        "graphql": "^16.8.1",
