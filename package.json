        "ts-loader": "^9.4.3",
        "ts-node": "^10.9.1",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.1.3",
        "unplugin-swc": "^1.4.5"
    },
    "packageManager": "yarn@4.1.1"
}
